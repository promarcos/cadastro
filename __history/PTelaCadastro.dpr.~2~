program PTelaCadastro;

uses
  System.StartUpCopy,
  FMX.Forms,
  UFrmTela in 'UFrmTela.pas' {Form1},
  UFrmLogin in 'UFrmLogin.pas' {Form2},
  UFrmCadastro in 'UFrmCadastro.pas' {Form3};

